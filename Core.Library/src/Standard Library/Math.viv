procedure Math.Int8_Clamp(value: int8, min: int8, max: int8) => int8 
{
    if (min > max)
    {
        print("MinMaxException: " + string(min) + " " + string(max));
    }

    if (value < min)
    {
        return min;
    }
    else if (value > max)
    {
        return max;
    }
    return value;
}

